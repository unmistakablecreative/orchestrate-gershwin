{
  "commands": {
    "Load OrchestrateOS": {
      "run": [
        {
          "tool": "${SAFE_DOMAIN}__jit_plugin",
          "action": "executeTask",
          "params": {
            "tool_name": "json_manager",
            "action": "read_json_file",
            "params": {"filename": "boot_flags.json"}
          }
        },
        {
          "tool": "${SAFE_DOMAIN}__jit_plugin",
          "action": "executeTask",
          "params": {
            "tool_name": "json_manager",
            "action": "read_json_file",
            "params": {"filename": "secondbrain.json"}
          }
        },
        {
          "tool": "${SAFE_DOMAIN}__jit_plugin",
          "action": "executeTask",
          "params": {
            "tool_name": "json_manager",
            "action": "read_json_file",
            "params": {"filename": "orchestrate_intent_routes.json"}
          }
        },
        {
          "tool": "${SAFE_DOMAIN}__jit_plugin",
          "action": "executeTask",
          "params": {
            "tool_name": "json_manager",
            "action": "read_json_file",
            "params": {"filename": "unlock_messages.json"}
          }
        },
        {
          "tool": "${SAFE_DOMAIN}__jit_plugin",
          "action": "executeTask",
          "params": {
            "tool_name": "json_manager",
            "action": "read_md_file",
            "params": {
              "filename": "orchestrate_os_protocol.md",
              "path": "system_docs"
            }
          }
        },
        {
          "tool": "${SAFE_DOMAIN}__jit_plugin",
          "action": "loadMemory"
        },
        {
          "tool": "${SAFE_DOMAIN}__jit_plugin",
          "action": "getSupportedActions"
        },
        {
          "tool": "${SAFE_DOMAIN}__jit_plugin",
          "action": "getDashboardFile",
          "params": {"file_key": "full_dashboard"}
        }
      ],
      "post_process": "render_dashboard_from_server",
      "display": {
        "style": "pre_formatted",
        "theme": "dark",
        "layout": "dashboard",
        "render_mode": "server_side"
      },
      "boot_flags_logic": {
        "onboarding_complete_false": {
          "additional_load": [
            {
              "tool": "${SAFE_DOMAIN}__jit_plugin",
              "action": "executeTask",
              "params": {
                "tool_name": "json_manager",
                "action": "read_json_file",
                "params": {"filename": "onboarding_sequence.json"}
              }
            }
          ],
          "behavior": "ALWAYS render dashboard first showing toolkit table. Then below dashboard, follow onboarding_sequence.json step by step starting at intro."
        },
        "onboarding_complete_true": {
          "behavior": "Render dashboard and await user commands."
        }
      },
      "instructions": {
        "rendering": "Display the pre-formatted dashboard data exactly as returned from the server. Render as normal text/markdown, NOT in code blocks. Show ALL dashboard sections including toolkit table.",
        "data_access": "All raw JSON data is available in the dashboard response for analysis and commands.",
        "display_format": "Show dashboard content as readable text with proper formatting, bullets, and headers. Never wrap dashboard output in code blocks.",
        "boot_flags_check": "Check boot_flags.json first. If onboarding_complete is false, render dashboard THEN show onboarding below it. If true, just render dashboard.",
        "context_files": "orchestrate_os_protocol.md provides system behavior rules. orchestrate_intent_routes.json provides command routing. unlock_messages.json provides guided_activation flows."
      },
      "prompt_behavior": "silent",
      "addendum": {
        "DISPLAY_SERVER_FORMATTED_OUTPUT": true,
        "NO_CLIENT_SIDE_RENDERING": true,
        "NO_JSON_TO_MARKDOWN_CONVERSION": true,
        "SHOW_PREFORMATTED_TABLES": true,
        "RAW_DATA_AVAILABLE_FOR_COMMANDS": true,
        "ALL_TOOL_REQUESTS_MUST_USE_EXECUTE_TASK": true,
        "CHECK_BOOT_FLAGS_BEFORE_ONBOARDING": true,
        "ALWAYS_RENDER_DASHBOARD_FIRST": true
      }
    }
  }
}